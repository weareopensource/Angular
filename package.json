{
  "name": "waos-angular",
  "description": "Full-Stack JavaScript with MongoDB / PostgreSQL, Express, AngularX, and Node.js.",
  "version": "0.0.0",
  "private": false,
  "authors": "https://github.com/weareopensource/waosAngular/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/waosAngular"
  },
  "engines": {
    "node": ">=6.3.0",
    "npm": ">=3.10.8"
  },
  "angular-cli": {},
  "scripts": {
    "check": "npm-check",
    "reinstall": "rimraf node_modules/ && npm cache clean && npm install",
    "start": "ng serve --proxy proxy.json --ssl 1 --ssl-key certs/key.pem --ssl-cert certs/cert.pem",
    "build:prod": "ng build --prod",
    "server": "http-server ./dist",
    "start:prod": "run-s build:prod server",
    "lint": "tslint \"client/**/*.ts\" --project client/tsconfig.app.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.e2e.json --type-check",
    "stylelint": "stylelint 'client/**/*.scss'",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc.1",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/flex-layout": "2.0.0-beta.10",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/entity": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "ngrx-store-freeze": "^0.2.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-check": "^5.5.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "ava": {
    "require": [
      "async-to-gen/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
